# Public API base URL used at build-time by Expo.
# Will be embedded into the JS bundle. Do NOT put secrets here.
# Precedence in code: EXPO_PUBLIC_API_URL > app.json expo.extra.apiUrl > hardcoded fallback
EXPO_PUBLIC_API_URL=http://50.114.185.206:8080

# API timeout in milliseconds (default: 15000ms = 15 seconds)
# Increase this if your server response time is slow
EXPO_PUBLIC_API_TIMEOUT_MS=60000
